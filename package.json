{
  "name": "3d-excalidraw-monorepo",
  "private": true,
  "packageManager": "pnpm@9",
  "version": "0.1.0",
  "scripts": {
    "dev": "concurrently -k -n web,server -c cyan,magenta \"pnpm --dir apps/web dev\" \"pnpm --dir apps/server dev\"",
    "build": "pnpm -r run build",
    "lint": "pnpm -r run lint",
    "typecheck": "pnpm -r run typecheck",
    "format": "prettier -w ."
  },
  "devDependencies": {
    "concurrently": "^9.0.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3"
  }
}
